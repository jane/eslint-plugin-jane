{
  "name": "eslint-plugin-jane",
  "description": "Jane's ESLint plugin and configurations",
  "version": "2.0.0",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "rules",
    "prettier.js"
  ],
  "scripts": {
    "format": "prettier \"*.js\" \"rules/*.js\" --write",
    "precommit": "lint-staged",
    "preversion": "npm t",
    "test": "run-s -s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/jane/eslint-plugin-jane#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jane/eslint-plugin-jane.git"
  },
  "bugs": "https://github.com/jane/eslint-plugin-jane/issues",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "jane",
    "lint",
    "plugin"
  ],
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-unicorn": "6.0.1"
  },
  "devDependencies": {
    "eslint": "5.12.0",
    "eslint-plugin-self": "1.1.0",
    "flow-bin": "0.89.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "run-s": "0.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "eslint": "^5.1.0"
  }
}
