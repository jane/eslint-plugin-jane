{
  "name": "eslint-plugin-jane",
  "description": "Jane's ESLint plugin and configurations",
  "version": "7.0.0",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "prettier-ts.js",
    "prettier.js",
    "rules"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier \"*.js\" \"rules/*.js\" --write && sortpack",
    "preversion": "npm t",
    "test": "run-s -s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/jane/eslint-plugin-jane#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jane/eslint-plugin-jane.git"
  },
  "bugs": "https://github.com/jane/eslint-plugin-jane/issues",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "jane",
    "lint",
    "plugin"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "babel-eslint": "10.0.3",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-unicorn": "13.0.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-plugin-self": "1.2.0",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "sortpack": "2.0.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "sortpack",
      "git add"
    ],
    "package.json": [
      "sortpack",
      "git add"
    ]
  },
  "peerDependencies": {
    "eslint": "^6.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
