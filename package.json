{
  "name": "eslint-plugin-jane",
  "description": "Jane's ESLint plugin and configurations",
  "version": "9.0.6",
  "author": "Jane",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "prettier-ts.js",
    "prettier.js",
    "rules"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier \"*.js\" \"rules/*.js\" --write && sortpack",
    "preversion": "npm t",
    "test": "run-s -s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/jane/eslint-plugin-jane#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jane/eslint-plugin-jane.git"
  },
  "bugs": "https://github.com/jane/eslint-plugin-jane/issues",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "jane",
    "lint",
    "plugin"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unicorn": "24.0.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-self": "1.2.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "sortpack": "2.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "package.json": [
      "sortpack"
    ]
  },
  "peerDependencies": {
    "eslint": "^7.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
